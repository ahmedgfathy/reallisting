{
  "name": "reallisting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "appwrite": "^16.0.2",
    "@vercel/analytics": "^1.5.0",
    "node-appwrite": "^14.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node -e \"const fs=require('fs'); if(fs.existsSync('dist')){fs.rmSync('dist',{recursive:true,force:true})}; fs.cpSync('build','dist',{recursive:true})\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup:appwrite": "node scripts/setup-appwrite-db.js",
    "migrate:appwrite": "node scripts/migrate-to-appwrite.js",
    "deploy:appwrite": "npm run build && node scripts/setup-appwrite-db.js",
    "dev:api": "node scripts/local-server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sharp": "^0.34.5",
    "dotenv": "^16.4.7"
  }
}